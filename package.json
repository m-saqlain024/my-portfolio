    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^14.0.0",
    "next": "latest",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.2.3",
    "next-theme": "^0.1.5",
    "node-html-parser": "^6.1.10",
