{
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "prepare": "husky install",
    "export": "next export",
    "nextsitemap": "next build && next-sitemap --config next-sitemap.config.js",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./pages --ignore-path .gitignore --max-warnings=0",
    "postinstall": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx)\"",
    "check-format": "yarn prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format && lint-staged",
    "pre-commit-lint": "yarn check-format && yarn lint-staged"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
