    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.30",
    "prettier": "^3.0.1",
    "react": "18.2.0",
    "react-datepicker": "^4.18.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-native-dropdown-picker": "^5.4.6",
    "react-player": "^2.13.0",
    "react-toastify": "^9.1.3",
    "sonner": "^0.7.4",
    "tailwindcss": "^3.3.3",
    "xml-js": "^1.6.11",
    "ytdl-core": "^4.11.5"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
